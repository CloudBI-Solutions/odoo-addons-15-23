<?xml version="1.0"?>
<odoo>
    <data>

<record id="estate_property_view_kanban" model="ir.ui.view">
    <field name="name">Rental kanban</field>
    <field name="model">rental.vehicle</field>
    <field name="arch" type="xml">
        <kanban>
            <field name="name"/>
            <field name="model_id"/>
            <field name="brand_id"/>
            <field name="model_year"/>
            <field name="reg_date"/>
            <templates>
                <t t-name="kanban-box">
                    <strong>
                        <div><field name="name"/></div>
                        <div><field name="brand_id"/></div>
                        <div><field name="model_year"/></div>
                        <div><field name="reg_date"/></div>
                    </strong>
                </t>
            </templates>
        </kanban>
    </field>
</record>
    </data>
</odoo>

